package coreJava;

public class content_another_string_java_Exercises {
	public static void main(String[] args) {
		String s = "Java Exercises";
		String str2 = "Java Exercise";
		
		System.out.println(s.concat(str2));
		
		System.out.println(s.endsWith("se"));
		System.out.println(str2.endsWith("se"));
		System.out.println("Dhruv virani to conacate a given string to the end of another string to compare a given string to the specified character seqence. Comparing topsint.com and and topsint.com : true computing Topsint.com and topsint.com : falses");
	}
}
