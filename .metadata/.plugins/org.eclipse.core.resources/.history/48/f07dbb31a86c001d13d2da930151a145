package coreJava;

import java.util.Scanner;

public class n_nn_nnn {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner (System.in);
		
		System.out.print("Input integer : ");
		int s  = sc.nextInt();
		
		int a,z;
		a = ((10*s)+s);
		z = ((100*s)+(10*a)+a);
		
		System.out.println("Computes the values : "+s+" + "+a+ " + "+z+"");
	}
}