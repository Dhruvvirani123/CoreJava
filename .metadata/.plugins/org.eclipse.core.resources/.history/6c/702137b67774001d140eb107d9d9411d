package jay_sir_class_work;

import java.util.Enumeration;
import java.util.Vector;

public class VectoreDemo {
	public static void main(String[] args) {
		Vector dh = new Vector<>();
		dh.add(1);
		dh.add('D');
		dh.add("Dhruv");
		dh.add(12.23);
		dh.add(-12);
		dh.add(1);
		dh.add(1212212.1212);
		
		System.out.println(dh);
		System.out.println();
		System.out.println("Size of Vector : \n"+dh.size());
		
		dh.remove(2);
		System.out.println(dh);
		System.out.println("Size of Vector : "+dh.size());
		
		Enumeration em = dh.elements();
		while(em.hasMoreElements()) {
			System.out.println(em.nextElement());
		}
	}
}
