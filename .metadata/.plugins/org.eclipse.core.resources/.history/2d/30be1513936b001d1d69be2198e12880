package coreJava;

public class pattern {
	public static void main(String[] args) {
		for(int i=1;i<=5;i++)
		{
			for(int j=1;j<=i;j++)
			{
				System.out.print(j);
			}
			System.out.println("");
		}
		System.out.println("");
		
		int k = 0;
		int j;
		for(int i=1;i<=10;i++)
		{
			for(j=1;j<=i;j++);
			{
				System.out.print(j);
				k++;
			}
			System.out.println("");
		}
	}
}
